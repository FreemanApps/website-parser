{
	"name": "website-parser",
	"version": "0.1.0",
	"type": "module",
	"scripts": {
		"dev": "wrangler dev --local --assets ./public --log-level",
		"dev:css": "tailwindcss -i ./app/app.css -o ./public/app.css --watch",
		"deploy": "tailwindcss -i ./app/app.css -o ./public/app.css --minify && wrangler deploy --assets ./public"
	},
	"dependencies": {
		"@hono/sentry": "^0.2.1",
		"@hono/zod-validator": "^0.1.3",
		"@postlight/parser": "^2.2.3",
		"hono": "^3.3.4",
		"marked": "^5.1.2",
		"xss": "^1.0.14",
		"zod": "^3.21.4"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20230724.0",
		"@types/marked": "^5.0.1",
		"@types/node": "^20.4.5",
		"@typescript-eslint/eslint-plugin": "^6.2.0",
		"@typescript-eslint/parser": "^6.2.0",
		"dotenv": "^16.3.1",
		"eslint": "^8.46.0",
		"prettier": "^3.0.0",
		"prettier-plugin-tailwindcss": "^0.4.1",
		"tailwindcss": "^3.3.3",
		"tsx": "^3.12.7",
		"wrangler": "^3.4.0"
	}
}
